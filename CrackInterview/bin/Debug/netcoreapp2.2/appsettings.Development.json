{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "AwdDB": "server=DESKTOP-BI0Q9IC; database=CrackInterview;Trusted_Connection=True;"
  },
  "ApplicationOptions": {
    "SwaggerJsonPath": "../{0}/swagger.json"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "F:\\Reddy\\New folder\\CrackInterview.json",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level}] {Message}{Properties}{NewLine}{Exception}",
          "fileSizeLimitBytes": "10000000",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": "true"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "CrackInterview"
    }
  }
}
